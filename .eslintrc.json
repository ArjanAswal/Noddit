{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true,
    "jest/globals": true
  },
  "rules": {
    "no-unused-vars": ["error", { "args": "none" }],
    "node/no-unpublished-require": 0
  },
  "plugins": ["jest", "security", "prettier"],
  "extends": [
    "eslint:recommended",
    "plugin:node/recommended",
    "plugin:security/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": "latest"
  }
}
